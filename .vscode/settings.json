{
	"editor.detectIndentation": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"explorerExclude.backup": null,
	"files.encoding": "utf8",
	"files.eol": "\r\n", // crlf
	"files.exclude": {
		"**/.classpath": true,
		"**/.DS_Store": true,
		"**/.factorypath": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.svn": true,
		"**/*.backup": true,
		"**/*.before": true,
		"**/*.dmb": true,
		"**/*.DS_Store": true,
		"**/*.int": true,
		"**/*.lk": true,
		"**/*.lnk": true,
		"**/*.log": true,
		"**/*.rsc": true,
		"**/CVS": true,
		"**/Thumbs.db": true
	},
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"gitblame.commitUrl": "https://github.com/Gesugao-san/GesuProject/commit/${hash}",
}
